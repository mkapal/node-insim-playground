{
  "name": "node-insim-playground",
  "version": "0.0.1",
  "description": "Node InSim Playground",
  "main": "index.js",
  "type": "module",
  "repository": "git@github.com:mkapal/node-insim-playground.git",
  "author": "Martin Kapal <flamecze@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:insim": "tsx watch src/insim/index.tsx",
    "start:outgauge": "tsx watch src/outgauge/index.tsx",
    "build:insim": "tsup src/insim/index.tsx",
    "build:outgauge": "tsup src/outgauge/index.tsx",
    "format": "prettier --write \"./**/*.{js,ts,md,json,yml,yaml}\"",
    "lint": "eslint \"./src/**/*\" --ext .js,.ts",
    "lint:fix": "eslint \"./src/**/*\" --ext .js,.ts --fix",
    "tsc": "tsc"
  },
  "dependencies": {
    "debug": "^4.4.0",
    "dotenv": "^16.5.0",
    "node-insim": "^4.6.3",
    "react": "^18.3.1",
    "react-node-insim": "^0.0.13",
    "supports-color": "^10.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.14.0",
    "@types/react": "18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
